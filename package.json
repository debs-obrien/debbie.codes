  "scripts": {
    "dev": "nuxt -o --port 8888",
    "dev:tests": "nuxt",
    "dev:host": "nuxt --hostname '192.168.0.199' --port 8888",
    "build": "nuxt build",
    "analyze": "nuxt build -a",
    "start": "nuxt start",
